{
    "name": "travel-u",
    "version": "1.0.0",
    "description": "Travel companion Electron app",
    "main": "out/main/index.js",
    "scripts": {
        "start": "electron-vite preview",
        "dev": "dotenv electron-vite dev",
        "dev:web": "concurrently \"vite --config vite.config.ts\" \"yarn dev:api\"",
        "dev:api": "dotenv ts-node -r tsconfig-paths/register src/main/web-server.ts",
        "build": "electron-vite build && cp -r config out/config",
        "build:main": "electron-vite build --main",
        "build:preload": "electron-vite build --preload",
        "build:renderer": "electron-vite build --renderer",
        "build:web": "vite build --config vite.config.ts",
        "clean": "rimraf dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BowieHe/travel-u.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/BowieHe/travel-u/issues"
    },
    "homepage": "https://github.com/BowieHe/travel-u#readme",
    "dependencies": {
        "@langchain/core": "^0.3.61",
        "@langchain/deepseek": "^0.0.2",
        "@langchain/google-genai": "^0.2.14",
        "@langchain/langgraph": "^0.3.6",
        "@modelcontextprotocol/sdk": "^1.15.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "cors": "^2.8.5",
        "dotenv": "^17.2.1",
        "express": "^5.1.0",
        "langchain": "^0.3.29",
        "lucide-react": "^0.536.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "devDependencies": {
        "@types/node": "^24.0.10",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "@vitejs/plugin-react": "^4.7.0",
        "concurrently": "^9.2.0",
        "dotenv-cli": "^10.0.0",
        "electron": "^27.0.0",
        "electron-vite": "^2.3.0",
        "rimraf": "^5.0.0",
        "tailwindcss": "^3.4.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3",
        "vite": "^5.0.0"
    },
    "packageManager": "yarn@4.9.0"
}