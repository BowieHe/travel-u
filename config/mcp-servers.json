{
	"mcpServers": {
		"fetch": {
			"type": "stdio",
			"command": "uvx",
			"args": ["mcp-server-fetch", "--ignore-robots-txt"]
		},
		"github": {
			"type": "stdio",
			"command": "docker",
			"args": [
				"run",
				"-i",
				"--rm",
				"-e",
				"GITHUB_PERSONAL_ACCESS_TOKEN",
				"swr.cn-north-4.myhuaweicloud.com/ddn-k8s/ghcr.io/github/github-mcp-server:latest"
			],
			"env": {
				"GITHUB_PERSONAL_ACCESS_TOKEN": "${PAT}"
			}
		},
		"time": {
			"type": "stdio",
			"command": "uvx",
			"args": ["mcp-server-time", "--local-timezone=Asia/Shanghai"]
		},
		"12306-mcp": {
			"type": "sse",
			"url": "https://mcp.api-inference.modelscope.net/55d69209ed514f/sse"
		},
		"variflight": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "@variflight-ai/variflight-mcp"],
			"env": {
				"X_VARIFLIGHT_KEY": "${VARIFLIGHT_KEY}"
			}
		},
		"amap-maps": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "@amap/amap-maps-mcp-server"],
			"env": {
				"AMAP_MAPS_API_KEY": "${AMAP_WEB_API}"
			}
		}
	}
}
